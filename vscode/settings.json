{
  "window.titleBarStyle": "custom",
  "editor.mouseWheelZoom": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "editor.fontFamily": "MesloLGS Nerd Font Propo",
  "terminal.integrated.fontFamily": "MesloLGS Nerd Font Propo",
  "editor.minimap.renderCharacters": false,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "cmake.configureOnOpen": true,
  "grunt.autoDetect": "on",
  "jake.autoDetect": "on",
  "editor.minimap.enabled": false,
  "editor.stickyScroll.enabled": false,
  "workbench.colorTheme": "Ayu Dark Lighter",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.lineNumbers": "on",
  "hediet.vscode-drawio.resizeImages": null,
  "diffEditor.renderSideBySide": true,
  "editor.wordWrap": "on",
  "editor.cursorSmoothCaretAnimation": "off",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "hediet.vscode-drawio.theme": "dark",
  "workbench.iconTheme": "material-icon-theme",
  "prettier.singleQuote": true,
  "editor.tabSize": 2,
  "editor.quickSuggestions": {
    "other": "off"
  },
  "editor.suggestOnTriggerCharacters": false,
  "headwind.classRegex": {
    "html": "\\bclass\\s*=\\s*[\\\"\\']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\']",
    "css": "\\B@apply\\s+([_a-zA-Z0-9\\s\\-\\:\\/]+);",
    "javascript": "(?:\\bclassName\\s*=\\s*[\\\"\\']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\'])|(?:\\btw\\s*`([_a-zA-Z0-9\\s\\-\\:\\/]*)`)",
    "javascriptreact": "(?:\\bclassName\\s*=\\s*[\\\"\\']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\'])|(?:\\btw\\s*`([_a-zA-Z0-9\\s\\-\\:\\/]*)`)",
    "typescript": "(?:\\bclassName\\s*=\\s*[\\\"\\']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\'])|(?:\\btw\\s*`([_a-zA-Z0-9\\s\\-\\:\\/]*)`)",
    "typescriptreact": "(?:\\bclassName\\s*=\\s*[\\\"\\']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\'])|(?:\\btw\\s*`([_a-zA-Z0-9\\s\\-\\:\\/]*)`)"
  },
  "vim.useSystemClipboard": true,
  "editor.cursorBlinking": "expand",
  "terminal.external.linuxExec": "warp-terminal"
}
